FLASK_ENV=development
# Database
DATABASE_URL=mysql+pymysql://user:pass@localhost:3306/db_ehrm

# Timezone & geofence
TIMEZONE=Asia/Makassar
DEFAULT_GEOFENCE_RADIUS=100

# Nextcloud configuration
# Base URL of your Nextcloud instance. Can be the instance root (e.g. https://host)
# or the full WebDAV URL. The application will resolve the correct path.
NEXTCLOUD_URL=
# Username for your Nextcloud account
NEXTCLOUD_USER=
# Password or app password for your Nextcloud account
NEXTCLOUD_PASS=
# Default folder to store uploads (optional)
NEXTCLOUD_DEFAULT_FOLDER=uploads

# Model configuration
MODEL_NAME=buffalo_l
SIGNED_URL_EXPIRES=604800
